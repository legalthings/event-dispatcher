{
  "env": {
    "doc": "The application environment.",
    "format": [
      "production",
      "development",
      "test",
      "lto"
    ],
    "default": "development",
    "env": "NODE_ENV"
  },
  "dispatcher": {
    "doc": "The config of this application, with a namespace so it can be merged into larger configs",
    "rabbitmq": {
      "client": {
        "doc": "The RabbitMQ client config",
        "default": {
          "protocol": "amqp",
          "hostname": "localhost",
          "port": "5672",
          "username": "guest",
          "password": "guest",
          "vhost": "/"
        }
      },
      "api": {
        "doc": "Url to get to RabbitMQ web api. Note that management plugin must be installed",
        "default": "http://localhost:15672/api"
      },
      "queue": {
        "doc": "The default queue name used by rabbitmq",
        "default": "''"
      },
      "exchange": {
        "doc": "The default exchange name used by rabbitmq",
        "default": "''"
      },
      "shovel": {
        "doc": "The default shovel name used by rabbitmq",
        "default": "default"
      }
    },
    "legalevents": {
      "url": {
        "default": "http://localhost:3030/api/events"
      }
    }
  }
}